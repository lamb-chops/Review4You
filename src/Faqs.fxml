            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../linkReview.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button contentDisplay="GRAPHIC_ONLY" layoutX="14.0" layoutY="375.0" mnemonicParsing="false" onAction="#navToPlaces" prefHeight="30.0" prefWidth="95.0" text="Places To Visit">
